{
  "name": "Guld Games",
  "version": "0.1.1",
  "description": "Guld Games",
  "permissions": ["clipboardRead", "clipboardWrite", "activeTab", "declarativeContent", "storage", "unlimitedStorage", "notifications", "nativeMessaging"],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Alt+Shift+G",
        "mac": "Alt+Shift+G",
        "chromeos": "Alt+Shift+G",
        "linux": "Alt+Shift+G"
      }
    }
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 2
}
